<h1>Hello World!</h1>

<%= link_to "Create User", new_user_path %><br/><hr/>

<% @userList.each do |user| %>

    <%= user.name %>
    <%= link_to 'Show', user %> | 
    <%= link_to 'Edit', edit_user_path(user) %> | 
    <%= link_to 'Delete', user, :confirm => 'Are you sure?', :method => :delete %>
    <br/><hr/>

<% end %>

